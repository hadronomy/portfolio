{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "~/*": ["./src/*"],
      "react": ["./node_modules/@types/react"]
    }
  },
  "exclude": ["node_modules", ".astro", "dist"]
}
